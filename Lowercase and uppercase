#include<stdio.h>
#include<string.h>
int main()
{
    char ch[10];
    int i,count,n;
    printf("Enter the String: \n");
    gets(ch);
    count = strlen(ch);
    
    for( i = 0; i < count; i++)
    {
        if(islower(ch[i]))
        {
            ch[i]=toupper(ch[i]);
                
        }
        else if(isupper(ch[i]))
        {
            ch[i]=tolower(ch[i]);
           
        }
        printf("%c",ch[i]);
     }
        return 0;
  
    
}
