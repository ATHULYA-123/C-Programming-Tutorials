#include <stdio.h>
IsPrime(int x)
 {
   int i,flag=1;
   for(i=2;i<=x/2;i++)
    {
      if(x%i==0)
      {
        flag=0;
        break;
      }
    }
   if(flag==1)
   {
     printf("It is a prime number\n",x);
   }
   else
   {
     printf("It is not a prime number\n",x);
   }
   
 }
 main()
 {
   int m;
   printf("Enter the number\n");
   scanf("%d",&m);
   IsPrime(m);
   
   getch();
 }
