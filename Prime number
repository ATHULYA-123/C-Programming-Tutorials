#include <stdio.h>
IsPrime(int n)
 {
int i;
if(n==1)
printf("Not a prime number");
else
{
for(i=2;i<n-1;i++)
  { 
  if(n%i==0)
 {
  printf("Not a prime number\n");
  return 0;
 }
  }
printf("Number is prime\n");
}
   
 }
 int main()
 {
   int m;
   printf("Enter the number\n");
   scanf("%d",&m);
   IsPrime(m);
   
   return 0;
 }
